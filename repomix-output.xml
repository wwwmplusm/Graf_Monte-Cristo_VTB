    setSelectedOffer(offerId);
  };

  const handleSubmitApplication = () => {
    setApplicationStatus('pending');
    // Simulate API call
    setTimeout(() => {
      setApplicationStatus(Math.random() > 0.3 ? 'approved' : 'declined');
    }, 2000);
  };

  if (applicationStatus === 'pending') {
