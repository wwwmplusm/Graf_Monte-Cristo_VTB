import json
import os
import time

import requests

# ================= КОНФИГУРАЦИЯ =================
CLIENT_ID = "team260"
CLIENT_SECRET = "r1VDOfJHciCpfxVw"
TEAM_ID = CLIENT_ID  # Глобальная переменная для заголовков

# Базы для выгрузки
BANKS = [
    ("vbank", "https://vbank.open.bankingapi.ru"),
    ("abank", "https://abank.open.bankingapi.ru"),
    ("sbank", "https://sbank.open.bankingapi.ru"),
]

TARGET_USERS = [f"{CLIENT_ID}-3", f"{CLIENT_ID}-10"]
OUTPUT_ROOT = "team260_data"
TX_PAGE_LIMIT = 100  # максимум по спецификации /accounts/{id}/transactions
WAIT_TIMEOUT = 300  # сек на ожидание одобрения согласия
CONSENT_POLL_INTERVAL = 10  # проверяем статус раз в 10 секунд

# Захардкоженные согласия, выданные вручную
HARDCODED_CONSENTS = {
    "team260-3": {
        "vbank": {"account": "consent-c942e1695ff4", "product": "pagc-0cf354edffdd"},
        "abank": {"account": "consent-30935697214b", "product": "pagc-0948e2026084"},
        "sbank": {"account": "consent-9061ccce4050", "product": "pagc-8808801a88c8"},
    },
    "team260-10": {
        "vbank": {"account": "consent-7c10a229bf68", "product": "pagc-538abe6111cc"},
        "abank": {"account": "consent-efe34a4db440", "product": "pagc-9b50ea04deb4"},
        "sbank": {"account": "consent-b4b60bf89a19", "product": "pagc-8808801a88c8"},
    },
}

session = requests.Session()

# ================= ВСПОМОГАТЕЛЬНЫЕ ФУНКЦИИ =================
def _read_json(path: str) -> dict:
    try:
        with open(path, "r", encoding="utf-8") as f:
            return json.load(f)
    except FileNotFoundError:
        return {}
    except Exception:
        return {}

def setup_directories(user_id: str, bank_slug: str) -> str:
    """Создает структуру папок под конкретный банк и пользователя"""
    root = os.path.join(OUTPUT_ROOT, user_id, bank_slug)
    folders = [
        f"{root}/profile",
        f"{root}/accounts",
        f"{root}/products",
        f"{root}/agreements"
    ]
    for folder in folders:
        os.makedirs(folder, exist_ok=True)
    print(f"📂 Директория данных: {root}")
    return root

def save_json(data, filename, folder, root):
    """Сохраняет JSON с красивыми отступами"""
    filepath = os.path.join(root, folder, filename)
    with open(filepath, 'w', encoding='utf-8') as f:
        json.dump(data, f, ensure_ascii=False, indent=4)
    print(f"   💾 Сохранено: {folder}/{filename}")


def _extract(data: dict, *keys):
    for key in keys:
        if key is None:
            continue
        if key in data:
            return data.get(key)
    return None


def wait_for_account_consent(token: str, base_url: str, root: str, request_id: str, initial_status: str | None) -> str | None:
    """Ждем одобрения account-consent по request_id/consent_id."""
    url = f"{base_url}/account-consents/{request_id}"
    headers = {
        "Authorization": f"Bearer {token}",
        "X-Requesting-Bank": CLIENT_ID,
    }
    last_status = initial_status
    start = time.time()
    print(f"   ⏳ Ожидание account consent {request_id} ...")
    while time.time() - start < WAIT_TIMEOUT:
        resp = session.get(url, headers=headers)
        if resp.status_code != 200:
            print(f"   ⚠️ Не получилось получить статус account-consent ({resp.status_code}): {resp.text}")
            break
        body = resp.json()
        consent_id = (
            _extract(body, "consent_id", "consentId")
            or _extract(body.get("data", {}), "consentId", "consent_id")
        )
        status = _extract(body, "status") or _extract(body.get("data", {}), "status")
        last_status = status or last_status
        if consent_id and status in ("approved", "active"):
            save_json(body, "account_consent.json", "profile", root)
            print(f"   ✅ Согласие (accounts) одобрено: {consent_id}")
            return consent_id
        time.sleep(CONSENT_POLL_INTERVAL)

    print(f"   ⚠️ Не дождались account consent, последний статус: {last_status}")
    return None


def wait_for_product_consent(token: str, base_url: str, root: str, request_id: str, initial_status: str | None, target_user_id: str) -> str | None:
    """Ждем одобрения product-agreement consent по request_id/consent_id."""
    url = f"{base_url}/product-agreement-consents/{request_id}"
    headers = {
        "Authorization": f"Bearer {token}",
    }
    params = {"client_id": target_user_id}

    last_status = initial_status
    start = time.time()
    print(f"   ⏳ Ожидание product consent {request_id} ...")
    while time.time() - start < WAIT_TIMEOUT:
        resp = session.get(url, headers=headers, params=params)
        if resp.status_code != 200:
            print(f"   ⚠️ Не получилось получить статус product-consent ({resp.status_code}): {resp.text}")
            break
        body = resp.json()
        consent_id = (
            _extract(body, "consent_id", "consentId")
            or _extract(body.get("data", {}), "consent_id", "consentId")
        )
        status = _extract(body, "status") or _extract(body.get("data", {}), "status")
        last_status = status or last_status
        if consent_id and status in ("approved", "active"):
            save_json(body, "product_consent.json", "profile", root)
            print(f"   ✅ Согласие (products) одобрено: {consent_id}")
            return consent_id
        time.sleep(CONSENT_POLL_INTERVAL)

    print(f"   ⚠️ Не дождались product consent, последний статус: {last_status}")
    return None

# ================= АВТОРИЗАЦИЯ И СОГЛАСИЯ =================

