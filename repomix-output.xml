#!/usr/bin/env python3
"""
Скрипт для полного сброса базы данных.
Удаляет существующий файл БД и создает новую чистую базу.
"""

import os
import sys
from pathlib import Path

# Add hktn to path
sys.path.insert(0, str(Path(__file__).parent))

from hktn.core.database import DB_FILE, init_db


def reset_database():
    """Удаляет существующую БД и создает новую."""
    db_path = Path(DB_FILE)
    
    # Удаляем файл БД, если существует
    if db_path.exists():
        print(f"🗑️  Удаляю существующую базу данных: {db_path.absolute()}")
        db_path.unlink()
        print("✅ База данных удалена")
    else:
        print(f"ℹ️  Файл базы данных не найден: {db_path.absolute()}")
    
    # Создаем новую БД
    print("🔧 Создаю новую базу данных с чистой схемой...")
    init_db()
    print("✅ База данных успешно создана!")
    print(f"📁 Расположение: {db_path.absolute()}")
    
    # Проверяем размер файла
    if db_path.exists():
        size_kb = db_path.stat().st_size / 1024
        print(f"📊 Размер файла: {size_kb:.2f} KB")


if __name__ == "__main__":
    print("=" * 60)
    print("🔄 СБРОС БАЗЫ ДАННЫХ")
    print("=" * 60)
    
    # Подтверждение (можно убрать для автоматического сброса)
    response = input("\n⚠️  Это удалит все данные! Продолжить? (yes/no): ")
    if response.lower() not in ['yes', 'y', 'да']:
        print("❌ Отменено")
        sys.exit(0)
    
    print()
    reset_database()
    print("\n" + "=" * 60)
    print("✨ Готово! База данных сброшена и готова к использованию.")
    print("=" * 60)
</file>

